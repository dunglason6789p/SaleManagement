    <script>
        function getData() {
            $.ajax({
                method: 'POST',
                url: '/SaleBill/SearchSaleBill',
                success: function (result) {
                    var value = "";
                    var tbody = document.getElementById("table-tbody");
                    for (var i = 0; i < result.length; i++) {
                        value += '<tr>';
                        value += '<td>';
                        value += '  <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">';
                        value += '  <input type="checkbox" class="group-checkable" data-set="#sample_2 .checkboxes" />';
                        value += '  </label>';
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['Code'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['DateCreated'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['StaffName'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['CustomerName'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['TotalValue']
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['DiscountValue']
                        value += '</td>';
                        value += "<td>"
                        value += '  <div class="margin-bottom-5">';
                        value += '      <button class="btn btn-sm btn-success filter-submit margin-bottom" ';
                        value += ' onclick="viewDetail(' + result[i]['ID'] + ')">';
                        value += '          <i class="fa fa-search"></i> View </button>';
                        value += '  </div>';
                        value += "</td>";
                        value += '</tr>';
                    }
                    tbody.innerHTML = value;
                },
                error: function (err) {
                    console.log(err);
                }
            });
        }
        function viewDetail(ID) {
            window.location.href = "/SaleBill/ViewDetail?ID=" + ID;
        }
        function search() {
            var code = document.getElementById("code").value;
            var dateFrom = document.getElementById("dateFrom").value;
            var dateTo = document.getElementById("dateTo").value;
            var customerName = document.getElementById("customerName").value;
            var staffName = document.getElementById("staffName").value;
            var totalValueFrom = document.getElementById("totalValueFrom").value;
            var totalValueTo = document.getElementById("totalValueTo").value;
            var discountFrom = document.getElementById("discountFrom").value;
            var discountTo = document.getElementById("discountTo").value;
            var url = '/SaleBill/SearchSaleBill?code=' + code;
            url += '&dateFrom=' + dateFrom;
            url += '&dateTo=' + dateTo;
            url += '&customerName=' + customerName;
            url += '&staffName=' + staffName;
            url += '&totalValueFrom=' + totalValueFrom;
            url += '&totalValueTo=' + totalValueTo;
            url += '&discountFrom=' + discountFrom;
            url += '&discountTo=' + discountTo;
            $.ajax({
                method: 'POST',
                url: url,
                success: function (result) {
                    var value = "";
                    var tbody = document.getElementById("table-tbody");
                    for (var i = 0; i < result.length; i++) {
                        value += '<tr>';
                        value += '<td>';
                        value += '  <label class="mt-checkbox mt-checkbox-single mt-checkbox-outline">';
                        value += '  <input type="checkbox" class="group-checkable" data-set="#sample_2 .checkboxes" />';
                        value += '  </label>';
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['Code'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['DateCreated'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['StaffName'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['CustomerName'];
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['TotalValue']
                        value += '</td>';
                        value += '<td>';
                        value += result[i]['DiscountValue']
                        value += '</td>';
                        value += "<td>"
                        value += '  <div class="margin-bottom-5">';
                        value += '      <button class="btn btn-sm btn-success filter-submit margin-bottom" ';
                        value += ' onclick="viewDetail(' + result[i]['Code'] + ')">';
                        value += '          <i class="fa fa-search"></i> View </button>';
                        value += '  </div>';
                        value += "</td>";
                        value += '</tr>';
                    }
                    tbody.innerHTML = value;
                },
                error: function (err) {
                    console.log(err);
                }
            });
        }
