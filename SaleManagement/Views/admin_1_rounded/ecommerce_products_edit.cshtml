                    <div class="row">
                        <div class="col-md-12">
                            <form class="form-horizontal form-row-seperated" id="form-data" action="/Product/CreateOrUpdate" method="post">
                                <div class="portlet">
                                    <div class="portlet-title">
                                        <div class="caption">
                                            <i class="fa fa-shopping-cart"></i> Product </div>
                                        <div class="actions btn-set">
                                            <input type="button" class="btn btn-secondary-outline" onclick="back()" value="Back" />
                                            <!--<button class="btn btn-secondary-outline" onclick="back()">
                                                <i class="fa fa-angle-left"></i> Back</button>-->
                                            <input type="button" class="btn btn-secondary-outline" onclick="window.location.reload();" value="Reset" />
                                            <!--<button class="btn btn-secondary-outline" onclick="reset()">
                                                <i class="fa fa-reply"></i> Reset</button>-->
                                            <button class="btn btn-success" onclick="save()">
                                                <i class="fa fa-check"></i> Save
                                            </button>
                                            <input type="button" class="btn btn-success" onclick="saveAndContinue()" value=" Save & Continue " />
                                            <input type="button" class="btn btn-danger" onclick="deleteProduct()" value="Delete" />
                                            
                                        </div>
                                    </div>
                                    <div class="portlet-body">
                                        <div class="tabbable-bordered">
                                            <ul class="nav nav-tabs">
                                                <li class="active">
                                                    <a href="#tab_general" data-toggle="tab"> General </a>
                                                </li>
                                            </ul>
                                            <div class="tab-content">
                                                <div class="tab-pane active" id="tab_general">
                                                    <div class="form-body">
                                                        <table class="table">
                                                            <tr>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Product's ID:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="Code"
                                                                                   placeholder="Product's ID..." value="@Model.Code">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Name:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="Name"
                                                                                   placeholder="Name..." value="@Model.Name">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            UnitName:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="UnitName"
                                                                                   placeholder="UnitName..." value="@Model.UnitName">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Retail Price:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="RetailPrice"
                                                                                   placeholder="Retail Price..." value="@Model.RetailPrice">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Wholesale Price:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="WholesalePrice"
                                                                                   placeholder="Wholesale Price..." value="@Model.WholesalePrice">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Wholesale Min Amount:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="WholesaleMinAmount"
                                                                                   placeholder="Wholesale Min Amount..." value="@Model.WholesaleMinAmount">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Average Cost:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="AverageCost"
                                                                                   placeholder="Average Cost..." value="@Model.AverageCost">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Discount Rate:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="DiscountRate"
                                                                                   placeholder="Discount Rate..." value="@Model.DiscountRate">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                            </tr>
                                                            <tr>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Availability:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <input type="text" class="form-control" name="Availability"
                                                                                   placeholder="Availability..." value="@Model.Availability">
                                                                        </div>
                                                                    </div>
                                                                </td>
                                                                <td>
                                                                    <div class="form-group">
                                                                        <label class="col-md-4 control-label">
                                                                            Category Name:
                                                                            <span class="required"> * </span>
                                                                        </label>
                                                                        <div class="col-md-8">
                                                                            <select class="form-control" name="CategoryName">
                                                                                @if (Model.CategoryName == "Đặc sản"){
                                                                                <option value="Đặc sản" selected>Đặc sản</option>
                                                                                } else {
                                                                                <option value="Đặc sản">Đặc sản</option>
                                                                                }
                                                                                @if (Model.CategoryName == "Ngũ cốc, sản phẩm từ ngũ cốc"){
                                                                                <option value="Ngũ cốc, sản phẩm từ ngũ cốc" selected>Ngũ cốc, sản phẩm từ ngũ cốc</option>
                                                                                } else {
                                                                                <option value="Ngũ cốc, sản phẩm từ ngũ cốc">Ngũ cốc, sản phẩm từ ngũ cốc</option>
                                                                                }
                                                                                @if (Model.CategoryName == "Thực phẩm chín, qua chế biến"){
                                                                                <option value="Thực phẩm chín, qua chế biến" selected>Thực phẩm chín, qua chế biến</option>
                                                                                } else {
                                                                                <option value="Thực phẩm chín, qua chế biến">Thực phẩm chín, qua chế biến</option>
                                                                                }
                                                                                @if (Model.CategoryName == "Thực phẩm tự nhiên từ động vật"){
                                                                                <option value="Thực phẩm tự nhiên từ động vật" selected>Thực phẩm tự nhiên từ động vật</option>
                                                                                } else {
                                                                                <option value="Thực phẩm tự nhiên từ động vật">Thực phẩm tự nhiên từ động vật</option>
                                                                                }
                                                                                @if (Model.CategoryName == "Thực phẩm tươi sống"){
                                                                                <option value="Thực phẩm tươi sống" selected>Thực phẩm tươi sống</option>
                                                                                } else {
