    <script>
        function getData(ID) {
            $.ajax({
                method: 'POST',
                url: '/SaleBill/GetSaleBillWithDetail_JSON?ID='+ID,
                success: function (result) {
                    var show_common_infomation_body = '<i class="icon-settings font-dark"></i>';
                    show_common_infomation_body += '<span class="caption-subject font-dark sbold uppercase"> Order #' + result['Code'];
                    show_common_infomation_body += '    <span class="hidden-xs">| ' + result['DateCreated']+' </span>';
                    show_common_infomation_body += '</span>';
                    document.getElementById('show-common-infomation').innerHTML = show_common_infomation_body;
                    document.getElementById('staff-body').innerHTML = makeStaffBody(result['Staff']);
                    document.getElementById('customer-body').innerHTML = makeCustomerBody(result['Customer']);
                    document.getElementById('order-body').innerHTML = makeOrderBody(result);
                    document.getElementById('order-detail-body').innerHTML = makeOrderDetailBody(result['SaleBillDetails']);
                    console.log(result);
                },
                error: function (err) {
                    console.log(err);
                }
            });
        }
        function makeStaffBody(staff) {
            var staff_body_value = '<div class="row static-info">';
            staff_body_value += '   <div class="col-md-5 name"> Name: </div >';
            staff_body_value += '   <div class="col-md-7 value">' + staff['FullName'] + '</div>';
            staff_body_value += '</div >';
            staff_body_value += '<div class="row static-info">';
            staff_body_value += '   <div class="col-md-5 name"> Email: </div>';
            staff_body_value += '   <div class="col-md-7 value">' + staff['Email'] + '</div>';
            staff_body_value += '</div>';
            staff_body_value += '<div class="row static-info">';
            staff_body_value += '   <div class="col-md-5 name"> Address: </div>';
            staff_body_value += '   <div class="col-md-7 value">' + staff['Address'] + '</div>';
            staff_body_value += '</div>';
            staff_body_value += '<div class="row static-info">';
            staff_body_value += '   <div class="col-md-5 name"> Phone Number: </div>';
            staff_body_value += '   <div class="col-md-7 value">' + staff['Phone'] + '</div>';
            staff_body_value += '</div>';
            return staff_body_value;
        }
        function makeCustomerBody(customer) {
            var customer_body_value = '<div class="row static-info">';
            customer_body_value += '    <div class="col-md-5 name"> Name: </div>';
            customer_body_value += '    <div class="col-md-7 value">' + customer['FullName'] + '</div>';
            customer_body_value += '</div>';
            customer_body_value += '<div class="row static-info">';
            customer_body_value += '    <div class="col-md-5 name"> Email: </div>';
            customer_body_value += '    <div class="col-md-7 value">' + customer['Email'] + '</div>';
            customer_body_value += '</div>';
            customer_body_value += '<div class="row static-info">';
            customer_body_value += '    <div class="col-md-5 name"> Address: </div>';
            customer_body_value += '    <div class="col-md-7 value">' + customer['Address'] + '</div>';
            customer_body_value += '</div>';
            customer_body_value += '<div class="row static-info">';
            customer_body_value += '    <div class="col-md-5 name"> Phone Number: </div>';
            customer_body_value += '    <div class="col-md-7 value">' + customer['Phone'] + '</div>';
            customer_body_value += '</div>';
            return customer_body_value;
        }
        function makeOrderBody(order) {
            var order_body_value = '<div class="row static-info">';
            order_body_value += '   <div class="col-md-5 name"> Order #: </div>';
            order_body_value += '   <div class="col-md-7 value">' + order['Code']+'</div>';
            order_body_value += '</div>';
            order_body_value += '<div class="row static-info">';
            order_body_value += '   <div class="col-md-5 name"> Order date: </div>';
            order_body_value += '   <div class="col-md-7 value">' + order['DateCreated']+'</div>';
            order_body_value += '</div>';
            order_body_value += '<div class="row static-info">';
            order_body_value += '   <div class="col-md-5 name"> Discount Value: </div>';
            order_body_value += '   <div class="col-md-7 value">' + order['DiscountValue']+'%</div>';
            order_body_value += '</div>';
            order_body_value += '<div class="row static-info">';
            order_body_value += '   <div class="col-md-5 name"> Total Value: </div>';
            order_body_value += '   <div class="col-md-7 value">' + makeNumber(order['TotalValue']) + '</div>';
            order_body_value += '</div>';
            return order_body_value;
        }
        function makeOrderDetailBody(saleBillDetail) {
            var billDetail;
            var product;
            var order_detail_body_value = '';
            for (var i = 0; i < saleBillDetail.length; i++) {
                billDetail = saleBillDetail[i];
                product = billDetail['Product'];
                order_detail_body_value += '<tr>';
                order_detail_body_value += '<td>' + product['Name'] + '</td>';
                if (product['Status'] == 0) {
                    order_detail_body_value += '<td> Còn hàng </td>';
                } else {
                    order_detail_body_value += '<td> Hết hàng </td>';
                }
                order_detail_body_value += '<td>' + makeNumber(billDetail['Price']) + '</td>';
                order_detail_body_value += '<td>' + makeNumber(billDetail['Price']) + '</td>';
                order_detail_body_value += '<td>' + billDetail['Amount'] + '</td>';
                order_detail_body_value += '<td>' + product['DiscountRate'] + '%</td>';
                order_detail_body_value += '<td>' + makeNumber(billDetail['Price'] * billDetail['Amount']) + '</td>';
                order_detail_body_value += '</tr>';
            }
            return order_detail_body_value;
        }
        function makeNumber(number) {
            var value = '';
            var count = 0;
            while (number >= 1) {
                count++;
                value = number % 10 + value;
                number = Math.round(number / 10);
                if (count % 3 == 0 && number >= 1) {
                    value = '.' + value;
                }
            }
            return value;
        }
    </script>
